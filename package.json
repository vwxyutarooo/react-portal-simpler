{
  "name": "react-portal-simpler",
  "version": "0.0.5",
  "description": "Simplest React portal wrapper component.",
  "main": "build/index.js",
  "files": [
    "lib",
    "build"
  ],
  "repository": {
    "url": "https://github.com/vwxyutarooo/react-portal-simpler",
    "type": "git"
  },
  "author": "vwxyutarooo",
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "develop": "NODE_ENV=development npm run develop:server",
    "develop:server": "babel-node tools/server.js",
    "develop:webpack": "webpack-dev-server --color --open http://localhost:8080 --hot --inline --content-base ./docs/ --config tools/webpack.config.develop.babel.js",
    "build": "mkdir -p build && babel ./lib --out-dir ./build",
    "build:sample": "NODE_ENV=production webpack --color --progress --config tools/webpack.config.production.babel.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-es2015-object-super",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.6.5",
    "babel-plugin-transform-es2015-object-super": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "css-loader": "0.25.0",
    "eslint": "3.9.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "6.6.0",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.8.0",
    "postcss-loader": "1.1.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "deep-assign": "^2.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack-merge": "^0.15.0"
  }
}
